{
	"Docker": {
		"UseEnvironmentVariables": false,
		"Endpoint" : "unix:///var/run/docker.sock",
		"Description": "The default Docker server on localhost"
	},
	"Pier": {
		"InternalServicesFolder": "../services/_internal"
	},
	"Server": {
		"Address": ":8443",
		"ReadTimeoutSecs": 10,
		"WriteTimeoutSecs": 10,
		"TLSCertificateFilePath": "../ssl/server.crt",
